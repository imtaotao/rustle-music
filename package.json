    "NeteaseCloudMusicApi": "^3.4.0",
    "apicache": "^1.2.1",
    "asar": "^1.0.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "download-git-repo": "^1.1.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.9.1",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "grass-loader": "^0.0.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.14.1",
