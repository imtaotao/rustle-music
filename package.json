{
  "name": "rustle",
  "version": "1.0.0",
  "music-version": "3.1.0",
  "description": "music application",
  "main": "client/main.js",
  "scripts": {
    "i": "rm -rf node_modules && yarn",
    "dev": "electron .",
    "icon": "node ./web/assets/change.js",
    "start": "webpack-dev-server --env.dev",
    "build": "webpack --env.build",
    "build:rm": "rm -rf ./dist && webpack --env.build",
    "build:an": "rm -rf ./dist && webpack --env.build --env.analyzer",
    "package:mac": "yarn build:rm && cross-env electron-builder --mac",
    "package:win": "yarn build:rm && cross-env electron-builder --win --ia32",
    "package:purewin": "cross-env electron-builder --win --ia32"
  },
  "author": "imtaotao",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "md5": "^2.3.0",
    "music-metadata": "^7.5.3",
    "pac-proxy-agent": "^4.0.0",
    "qrcode": "^1.4.4",
    "tunnel": "^0.0.6",

    "@rustle/grass": "^0.0.1-alpha.8",
    "@rustle/hearken": "^0.0.1-alpha.4",
    "cookie": "^0.3.1",
    "input-range": "^1.2.8",
    "opn": "^5.4.0",
    "request": "^2.85.0",
    "three": "^0.101.1",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
